        {{#if nopop}}
        {{#each nopop}}
        <div class="row">
          <div class="col-md-12">
            <div class="card ">
              <div class="card-header ">
                <h5 class="card-title">Payment Details</h5>
               
              </div>
              <div class="card-body ">
                
                <p class = "text-danger"><b>
                Pay {{nopop.payer_fullname}} N1000 before {{nopop.expire}}.</b>
                </p>
                <br>
                <p>
                Account Name:  {{nopop.payer_account_name}}<br>
                Account Number: {{nopop.payer_account_number}}<br>
                Bank: {{nopop.payer_bank_name}}<br>
                Amount: N1000<br>
                Phone: {{nopop.payer_phone}}<br>
                Status: {{nopop.status}}<br>
                Time Left: <span class = "text-danger"> {{timeleft}} .</span>
                </p>
               
               
                
                	<div class = "form-group">
                	<a href = "/iPaid/{{nopop.order_id}}"><button class = "btn btn-primary" type= "button">I have paid</button></a>
                	</div>
                
              </div>
              <div class="card-footer ">
                <hr>
                <div class="stats">
                  <i class="fa fa-history"></i> Updated 3 minutes ago
                </div>
              </div>
            </div>
          </div>
        </div>
        {{/each}}
        {{/if}}
        </div>
